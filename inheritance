#write a inheritance prog (take eg of corona)
class Corona():
    def __init__(self):
        print("Corona is virus")
    def fever(self):
        print("symotoms of corona - fever")
    def cough(self):
        print("symotoms of corona - cough")
    def lossOfSmell(self):
        print("symotoms of corona - lossOfSmell")
class coronaNewstrain(Corona):
    def __init__(self):
        print("coronaNewstrain is a change variant of covid")
    def headache(self):
        print("symotoms of coronaNewstrain-Headache")
    def skin_Rashes(self):
        print("symotoms of coronaNewstrain-skin_Rashes")
c=coronaNewstrain()
c.fever()
c.cough()
c.lossOfSmell()
c.headache()
c.skin_Rashes()
